<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- UserRegisterVO 클래스의 bean을 만든다. -->
	<bean id="userRegisterVO" class="com.foodary.vo.UserRegisterVO"/>
	
	<!-- UserRegisterList 클래스의 bean을 만든다. -->
	<bean id="userRegisterList" class="com.foodary.vo.UserRegisterList"/>
	
	<!-- UserRegisterDAO 클래스의 bean을 만든다. -->
	<bean id="userRegisterDAO" class="com.foodary.dao.UserRegisterDAO"/>

	
</beans>




